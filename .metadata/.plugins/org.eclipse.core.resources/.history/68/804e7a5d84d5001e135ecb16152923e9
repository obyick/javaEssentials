package entities;

import java.sql.Date;

public class Order {
	private Date date;
	private Inventory product;
	
	public Order(Date date, Inventory product) {
		this.date = date;
		this.product = product;
	}

	public Date getDate() {
		return date;
	}

	public void setDate(Date date) {
		this.date = date;
	}

	public Inventory getProduct() {
		return product;
	}

	public void setProduct(Inventory product) {
		this.product = product;
	}
	
}
